{
  "name": "react-time-tracker",
  "version": "1.0.0",
  "description": "Simple react app",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --content-base public/ --config config/webpack.dev.config.js",
    "build": "NODE_ENV=production webpack -p --config config/webpack.prod.config.js",
    "test": "NODE_ENV=test jest",
    "test-debug": "NODE_ENV=test node $NODE_DEBUG_OPTION --inspect ./node_modules/.bin/jest --runInBand",
    "test-coverage": "NODE_ENV=test jest --coverage"
  },
  "author": "Hendrik Schaeidt",
  "license": "MIT",
  "dependencies": {
    "@types/jquery": "^2.0.40",
    "@types/react": "^15.0.12",
    "@types/react-dom": "^0.14.23",
    "@types/react-router": "^3.0.5",
    "foundation-sites": "^6.2.0",
    "jquery": "^2.2.1",
    "moment": "^2.17.0",
    "node-uuid": "^1.4.7",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "@types/react-test-renderer": "^15.4.2",
    "awesome-typescript-loader": "^3.0.7",
    "babel-core": "^6.5.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "sass-loader": "^4.0.2",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.0",
    "ts-jest": "^19.0.0",
    "typescript": "^2.2.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.0.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/app.jsx"
    ]
  }
}
